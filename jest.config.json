{
  "collectCoverage": true,
  "coverageDirectory": "./coverage",
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/.assets/scripts/markdown-decorator/__tests__/"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  },
  "testPathIgnorePatterns": [
    "/__tests__/__fixtures__/"
  ],
  "verbose": true
}
